{
    "name": "brokencrust/deletemyaccount",
    "type": "phpbb-extension",
    "description": "Adds an option under the profile tab of the UCP that allows memebers to delete their own account. This is different than the version at phpBB.",
    "homepage": "https://brokencrust.com/exts/delete-my-account",
    "version": "1.0.0-a",
    "time": "2020-06-08",
    "license": "GPL-2.0-only",
    "authors": [
        {
            "name": "BrokenCrust",
            "email": "bc@brokencrust.com",
            "homepage": "https://brokencrust.com",
            "role": "Developer"
        },
        {
            "name": "RMcGirr83",
			"homepage": "https://github.com/rmcgirr83?tab=repositories",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.3.5",
        "composer/installers": "~1.0",
        "phpbb/phpbb": ">=3.2.0,<3.3.0@dev"
    },
    "extra": {
        "display-name": "Delete My Account",
        "soft-require": {
            "phpbb/phpbb": ">=3.2.0,<3.3.0@dev"
        },
        "version-check": {
            "host": "www.phpbb.com",
            "directory": "/customise/db/extension/delete_my_account_2",
            "filename": "version_check"
        }
    }
}